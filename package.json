{
  "name": "time-tracker-9000",
  "version": "1.0.0",
  "description": "A command-line time tracking application",
  "main": "index.js",
  "bin": {
    "timetrack": "./index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pkg . --out-path dist",
    "build:all": "pkg . --targets node18-linux-x64,node18-macos-x64,node18-macos-arm64,node18-win-x64 --out-path dist"
  },
  "keywords": [
    "time",
    "tracking",
    "cli",
    "timesheet"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@yao-pkg/pkg": "^6.6.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "inquirer": "^8.2.6"
  },
  "pkg": {
    "scripts": [
      "index.js"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-macos-arm64",
      "node18-win-x64"
    ],
    "outputPath": "dist"
  }
}
